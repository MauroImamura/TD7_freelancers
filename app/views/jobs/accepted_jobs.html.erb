<h1 style="maring:50px">Sua lista de propostas aceitas</h1>
<% if @applications.empty? %>
    <p style="font-size: 20px; padding:15px">Você ainda não possui projetos em sua lista.</p>
<% else %>
    <% @applications.each do |applies| %>
        <% if applies.accepted? %>
            <p style="font-size: 20px; padding:15px"><%= link_to applies.job.title, job_path(applies.job) %></p>
        <% end %>
    <% end %>
<% end %>
<p style="font-size: 20px; padding:15px"><%= link_to 'Encontre projetos', job_list_path %></p>